NODE_ENV=development
PORT=3000
JSON_LIMIT=1mb

# F0 Engine
F0_SAFE_THRESHOLD=0.5
F0_SYNERGY_SCALE=0.05
F0_CACHE_MAX=1000
F0_CACHE_TTL_MS=300000
F0_REDIS_CACHE=0

# Admin API
ADMIN_API_KEYS=devkey123

# CORS
ALLOWED_ORIGINS=http://localhost:3000

# Suggestions AI verify
VERIFY_SUGGESTIONS=0
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini

# Datasheet PDF (optional S3)
DS_AI_COLUMNS=0
DS_S3_BUCKET=
DS_S3_CDN=

# Redis
REDIS_URL=redis://localhost:6379

# HTTP timeouts
HTTP_KEEPALIVE_MS=55000
HTTP_HEADERS_TIMEOUT_MS=60000
HTTP_REQUEST_TIMEOUT_MS=60000
