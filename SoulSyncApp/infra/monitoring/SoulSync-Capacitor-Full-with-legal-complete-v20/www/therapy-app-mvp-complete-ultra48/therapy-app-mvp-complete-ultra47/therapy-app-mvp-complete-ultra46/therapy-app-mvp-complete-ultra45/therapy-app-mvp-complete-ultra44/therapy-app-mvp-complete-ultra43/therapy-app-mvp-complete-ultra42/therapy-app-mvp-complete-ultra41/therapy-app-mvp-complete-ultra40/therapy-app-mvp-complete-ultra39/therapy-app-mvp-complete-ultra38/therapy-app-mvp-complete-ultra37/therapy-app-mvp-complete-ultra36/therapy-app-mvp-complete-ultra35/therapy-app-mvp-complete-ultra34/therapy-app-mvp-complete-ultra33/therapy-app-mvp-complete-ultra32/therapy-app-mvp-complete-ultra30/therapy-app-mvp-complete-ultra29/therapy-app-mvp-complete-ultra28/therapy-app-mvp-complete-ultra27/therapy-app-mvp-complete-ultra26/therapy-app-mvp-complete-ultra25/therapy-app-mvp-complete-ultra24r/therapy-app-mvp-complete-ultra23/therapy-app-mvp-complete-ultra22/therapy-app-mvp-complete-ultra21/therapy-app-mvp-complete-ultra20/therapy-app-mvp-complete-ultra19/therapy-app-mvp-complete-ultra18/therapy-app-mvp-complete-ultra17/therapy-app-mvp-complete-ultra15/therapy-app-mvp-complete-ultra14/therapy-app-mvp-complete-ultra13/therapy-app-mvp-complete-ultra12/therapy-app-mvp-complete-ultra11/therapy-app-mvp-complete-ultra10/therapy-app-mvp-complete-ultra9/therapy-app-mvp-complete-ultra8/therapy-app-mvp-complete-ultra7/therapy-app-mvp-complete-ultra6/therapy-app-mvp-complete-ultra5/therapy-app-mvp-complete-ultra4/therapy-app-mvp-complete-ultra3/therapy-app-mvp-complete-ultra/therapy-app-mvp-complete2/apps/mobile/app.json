{
  "expo": {
    "name": "Therapy App",
    "slug": "therapy-app",
    "scheme": "therapyapp",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.therapyapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.therapyapp"
    },
    "plugins": [
      "expo-notifications",
      "sentry-expo",
      "expo-secure-store",
      "expo-local-authentication"
    ],
    "extra": {
      "sentryDsn": "YOUR_SENTRY_DSN_HERE"
    }
  }
}