    "react-native-paper": "^5.12.5",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "~3.31.1",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/native-stack": "^6.9.18",
    "axios": "^1.6.7",
    "expo-print": "~12.8.0",
    "expo-sharing": "~11.10.0",
